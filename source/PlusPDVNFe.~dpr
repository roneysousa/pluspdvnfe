     Application.CreateForm(TdmDados, dmDados);
  Application.CreateForm(TdmNFe, dmNFe);
  Application.CreateForm(TFrmMainNFe, FrmMainNFe);
  frmLogin := TfrmLogin.Create(nil);
     if frmLogin.ShowModal = mrOk then
      begin
           FreeAndNil(frmLogin);
           Application.Run;
      end
      else
      begin
           FreeAndNil(dmNFe);
           FreeAndNil(dmDados);
